{
    "name": "Getting started",
    "description": "Building an extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "minimum_chrome_version": "93",
    "background":{
        "service_worker":"eventPage.js",
        "type": "module"
    },
    "permissions": [
        "storage",
        "contextMenus",
        "tabs",
        "activeTab"
    ],
    "host_permissions": ["https://api.clickup.com/*"]
    
}